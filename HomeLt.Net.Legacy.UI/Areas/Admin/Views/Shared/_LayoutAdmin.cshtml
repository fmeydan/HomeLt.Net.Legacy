@{ 
    var user = Session[HomeLt.Net.Legacy.UI.Constants.Sessions.SessionAdmin] as HomeLt.Net.Legacy.ENTITIES.AdminUser;

}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 3 | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">*@
    <!-- iCheck -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/icheck-bootstrap/icheck-bootstrap.min.css">*@
    <!-- JQVMap -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/jqvmap/jqvmap.min.css">*@
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">*@
    <!-- Daterange picker -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/daterangepicker/daterangepicker.css">*@
    <!-- summernote -->
    @*<link rel="stylesheet" href="~/Areas/Admin/Temp/adminlte/plugins/summernote/summernote-bs4.css">*@
    <!-- Google Font: Source Sans Pro -->
    <link href="~/Areas/Admin/Temp/adminlte/plugins/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Temp/adminlte/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css">
    @RenderSection("HomeListCss", false)
    
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="index3.html" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Contact</a>
                </li>
            </ul>

            <!-- SEARCH FORM -->
            <form class="form-inline ml-3">
                <div class="input-group input-group-sm">
                    <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search">
                    <div class="input-group-append">
                        <button class="btn btn-navbar" type="submit">
                            <i class="fas fa-search"></i>
                        </button>
                    </div>
                </div>
            </form>

            <!-- Right navbar links -->

        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Admin/Main/Index/" class="brand-link">
                <img src="~/Areas/Admin/Temp/adminlte/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3"
                     style="opacity: .8">
                <span class="brand-text font-weight-light">HomeLT</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">@user.FirstName @user.LastName</a>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                        with font-awesome or any other icon font library -->


                        <li class="nav-item has-treeview">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-copy"></i>
                                <p>
                                    Property Options
                                    <i class="fas fa-angle-left right"></i>
                                  
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("List","Property")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Property List</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("Add","Property")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Add Home</p>
                                    </a>
                                </li>
                               
                            </ul>
                        </li>



                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>
    <!-- ./wrapper -->
    <!-- jQuery -->
    <script src="~/Areas/Admin/Temp/adminlte/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Areas/Admin/Temp/adminlte/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/Areas/Admin/Temp/adminlte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/chart.js/Chart.min.js"></script>*@
    <!-- Sparkline -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/sparklines/sparkline.js"></script>*@
    <!-- JQVMap -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/jqvmap/jquery.vmap.min.js"></script>*@
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>*@
    <!-- jQuery Knob Chart -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/jquery-knob/jquery.knob.min.js"></script>*@
    <!-- daterangepicker -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/moment/moment.min.js"></script>*@
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/daterangepicker/daterangepicker.js"></script>*@
    <!-- Tempusdominus Bootstrap 4 -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>*@
    <!-- Summernote -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/summernote/summernote-bs4.min.js"></script>*@
    <!-- overlayScrollbars -->
    @*<script src="~/Areas/Admin/Temp/adminlte/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>*@
    <!-- AdminLTE App -->
    <script src="~/Areas/Admin/Temp/adminlte/dist/js/adminlte.js"></script>
    <script src="~/Areas/Admin/Temp/adminlte/plugins/sweetalert2/sweetalert2.min.js"></script>

    <script src="~/Areas/Admin/Temp/adminlte/dist/js/ms.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    @RenderSection("HomeListJs", false)
    @RenderSection("EditPropertyJs",false)
</body>
</html>
